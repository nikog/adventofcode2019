open Jest;
open Expect;

describe("day01", () => {
  test("part1 test", () => {
    let input = [|".#..#", ".....", "#####", "....#", "...##"|];
    let (coord, _) = Day10.Part1.make(input);

    coord |> expect |> toBe("3,4");
  });

  test("part1 test", () => {
    let input = [|
      "......#.#.",
      "#..#.#....",
      "..#######.",
      ".#.#.###..",
      ".#..#.....",
      "..#....#.#",
      "#..#....#.",
      ".##.#..###",
      "##...#..#.",
      ".#....####",
    |];
    let (coord, _) = Day10.Part1.make(input);

    coord |> expect |> toBe("5,8");
  });

  test("part2 test", () => {
    let input = [|
      ".#..##.###...#######",
      "##.############..##.",
      ".#.######.########.#",
      ".###.#######.####.#.",
      "#####.##.#.##.###.##",
      "..#####..#.#########",
      "####################",
      "#.####....###.#.#.##",
      "##.#################",
      "#####.##.###..####..",
      "..######..##.#######",
      "####.##.####...##..#",
      ".#####..#.######.###",
      "##...#.##########...",
      "#.##########.#######",
      ".####.#.###.###.#.##",
      "....##.##.###..#####",
      ".#.#.###########.###",
      "#.#.#.#####.####.###",
      "###.##.####.##.#..##",
    |];
    let (coord, _) = Day10.Part1.make(input);

    coord |> expect |> toBe("11,13");
  });

  test("part2 test", () => {
    let input = [|
      ".#..##.###...#######",
      "##.############..##.",
      ".#.######.########.#",
      ".###.#######.####.#.",
      "#####.##.#.##.###.##",
      "..#####..#.#########",
      "####################",
      "#.####....###.#.#.##",
      "##.#################",
      "#####.##.###..####..",
      "..######..##.#######",
      "####.##.####...##..#",
      ".#####..#.######.###",
      "##...#.##########...",
      "#.##########.#######",
      ".####.#.###.###.#.##",
      "....##.##.###..#####",
      ".#.#.###########.###",
      "#.#.#.#####.####.###",
      "###.##.####.##.#..##",
    |];
    let coords = Day10.Part2.make(input);

    let (ax, ay) = coords[2];

    (ax |> string_of_int)
    ++ ","
    ++ (ay |> string_of_int)
    |> expect
    |> toBe("12,2");
  });
});
